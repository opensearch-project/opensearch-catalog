                            "ignore_above":32
                        },
                        "boundary_id":{
                            "type":"integer"
                        },
                        "uid":{
                            "type":"keyword",
                            "ignore_above":64
                        },
                        "direction":{
                            "type":"keyword",
                            "ignore_above":32
                        },
                        "direction_id":{
                            "type":"integer"
                        },
                        "protocol_ver":{
                            "type":"keyword",
                            "ignore_above":32
                        },
                        "protocol_ver_id":{
                            "type":"integer"
                        },
                        "protocol_name":{
                            "type":"keyword",
                            "ignore_above":256
                        },
                        "protocol_num":{
                            "type":"integer"
                        },
                        "session":{
                            "properties":{
                                "uid_alt":{
                                    "type":"keyword",
                                    "ignore_above":64
                                },
                                "count":{
                                    "type":"integer"
                                },
                                "created_time":{
