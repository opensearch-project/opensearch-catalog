                                },
                                "is_remote":{
                                    "type":"boolean"
                                },
                                "terminal":{
                                    "type":"keyword",
                                    "ignore_above":36
                                },
                                "uuid":{
                                    "type":"keyword",
                                    "ignore_above":64
                                },
                                "uid":{
                                    "type":"keyword",
                                    "ignore_above":64
                                },
                                "credential_uid":{
                                    "type":"keyword",
                                    "ignore_above":64
                                },
                                "is_vpn":{
                                    "type":"boolean"
                                }
                            }
                        },
                        "tcp_flags":{
                            "type":"integer"
                        }
