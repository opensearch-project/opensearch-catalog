{
    "template":{
        "mappings":{
            "properties":{
                "query_info":{
                    "properties":{
                        "data":{
                            "type":"text"
                        },
                        "name":{
                            "type":"keyword",
                            "ignore_above":64
                        },
                        "query_string":{
                            "type":"keyword",
                            "ignore_above":64
                        },
                        "query_time":{
                            "type":"date",
                            "format":"yyyy-MM-dd'T'HH:mm:ss.SSSZ||epoch_millis"
                        },
                        "query_time_dt":{
                            "type":"date",
                            "format":"yyyy-MM-dd'T'HH:mm:ss.SSSZ||epoch_millis"
                        },
                        "bytes":{
                            "type":"long"
                        },
                        "uid":{
                            "type":"keyword",
                            "ignore_above":64
                        }
                    }
                }
            }
        }
    }
}
